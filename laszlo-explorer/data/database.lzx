<canvas>
  <dataset name="dset" 
           request="true" type="http" 
           src="getemployees.jsp"/>
  
  <simplelayout axis="y"/>
  
  <view datapath="dset:/phonebook/employee">
    <simplelayout axis="x"/>
    <text datapath="firstName/text()"/>
    <text datapath="lastName/text()"/>
    <text datapath="phone/text()"/>
  </view>
</canvas>
