  
<canvas width="100%" height="200" proxied="false">
      <drawview height="200" width="200">
        <handler name="oncontext">
         this.moveTo(100,100);
         this.lineTo(200,100);
         this.quadraticCurveTo(120, 200, 300, 100)
         this.closePath()
         this.stroke()
         this.beginPath()
         this.moveTo(200,000);
         this.lineTo(100,200);
         this.quadraticCurveTo(120, 200, 300, 100)
         this.closePath()
         this.stroke()
        </handler>
      </drawview>
    </canvas>
