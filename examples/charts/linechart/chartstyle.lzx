                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
                 <labelstyle name="tip" font="serif" fontsize="10" fontcolor="0x000000"
                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
          </valuepointstyle>          
          <valuelinestyle name="valuelinestyle">
                 <linestyle name="line" color="0x0000FF" size="3" opacity="1"/>
                 <labelstyle name="label" font="myverity" fontsize="8" fontcolor="0x009933" linecolor="0x0000FF" fillcolor="0xFFFFC1" linesize="2"/>
                 <labelstyle name="tip" font="serif" fontsize="10" fontcolor="0x000000"  fillcolor="0xFFFFC1" />
          </valuelinestyle>
          <valueregionstyle name="valueregionstyle">
              <linestyle name="line" color="0x000000" size="1" opacity="1" />
              <regionstyle name="region" color="0x6699FF" opacity="0.5"/>
              <labelstyle name="label" font="serif" fontsize="12" fontcolor="0x000000"/>
              <labelstyle name="tip" font="serif" fontsize="12" fontcolor="0x000000"/>
          </valueregionstyle>
          <datastylelist name="datastyles">
               <datastyle>
                    <pointstyle name="point" width="10" height="10" color="0x66B3FF" shape="circle" opacity="0.5" />
                    <linestyle name="line" color="0x808080" size="3" opacity="0"/>
                    <regionstyle name="region" color="0xff0000" opacity="0.8" gradient="radial"/>
                    <labelstyle name="label" font="serif" fontsize="9" fontcolor="0x000000" linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2" fontangle="90"/>
                    <labelstyle name="tip" font="serif" fontsize="8" fontcolor="0x000000"
                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
               </datastyle>
               <datastyle>
                    <pointstyle name="point" width="10" height="10" color="0xFF0033" shape="triangle" opacity="0.8"/>
                    <linestyle name="line" color="0xFF6666" size="2" opacity="1" />
                    <regionstyle name="region" color="0x00FF00FF" opacity="0.8" />
                    <labelstyle name="label" font="serif" fontsize="9" fontcolor="0x000000"
                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2" fontangle="45"/>
                    <labelstyle name="tip" font="serif" fontsize="10" fontcolor="0x000000"
                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
               </datastyle>
               <datastyle>
                    <pointstyle name="point" width="6" height="6" color="0x00FF00" opacity="1"/>
                    <linestyle name="line" color="0xAA99DD" size="3" opacity="1"/>
                    <regionstyle name="region" color="0xB71CB3" opacity="0.8" gradient="none" />
                    <labelstyle name="label" font="myverity" fontsize="8" fontcolor="0x000000"
                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2" fontangle="45"/>
                    <labelstyle name="tip" font="serif" fontsize="10" fontcolor="0x000000"
                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
               </datastyle>
          </datastylelist>
     </chartstyle>
     
