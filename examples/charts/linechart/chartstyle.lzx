               <linestyle name="gridline" color="0x000000" size="1"/>
          </axisstyle>          
          <valuepointstyle name="valuepointstyle">
                <pointstyle name="point" width="15" height="15" color="0x009900" opacity="0.5" shape="triangle"/>
