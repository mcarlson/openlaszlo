<library>  
    <include href="charts/styles/chartstyle.lzx"/>
<!--- chartstyle01 used for test_line_01.lzx-->             

     <chartstyle name="chartstyle01">
          <plotstyle name="plot" linesize = "8" linecolor = "0x996666" fillcolor = "0x969696" opacity = "0.8"/>
          <chartbgstyle name="chartbgstyle">
              <linestyle name="line" size="3"/>
              <regionstyle name="region" color="0xFFFF99" opacity="0.4"/>
          </chartbgstyle>          
          <axisstyle name="haxisstyle">
               <labelstyle name="label" font="myverity" fontsize="16" fontcolor="0xFF9900" fontangle="90"/>
               <tickstyle name="tickmajor" color="0x000000" position="outside" length="10" size="1">
                    <labelstyle name="label" font="serif" fontsize="10" fontcolor="0x000000" linesize="2"/>
               </tickstyle>
               <tickstyle name="tickminor" color="0x000000" position="outside" length="5" size="1"/>
            <linestyle name="axisline" color="0x000000" size="2" opacity="0"/>
            <linestyle name="gridline" color="0x000000" size="1" />
          </axisstyle>
          <axisstyle name="vaxisstyle" >
               <labelstyle name="label" font="myverity" fontsize="16" fontcolor="0xFF9900" fontangle="90" linesize="1"/>
               <tickstyle name="tickmajor" color="0xFF0000" position="outside" length="10" size="1">
                    <labelstyle name="label" font="serif" fontsize="12" fontcolor="0x000000" fontangle="180"/>
               </tickstyle>
               <tickstyle name="tickminor" color="0x000000" position="outside" length="5" size="1"/>
               <linestyle name="axisline" color="0x000000" size="2" />
               <linestyle name="gridline" color="0x009900" size="2"/>
          </axisstyle>
          <valuepointstyle name="valuepointstyle">
              <pointstyle name="point" width="20" height="20" opacity="0" />
              <labelstyle name="label" font="myverity" fontsize="7" fontcolor="0x000000" linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
              <labelstyle name="tip" font="myverity" fontsize="8" fontcolor="0x000000" linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
          </valuepointstyle>
          <valuelinestyle name="valuelinestyle">
                <linestyle name="line" color="0xFF9999" size="2" opacity="1"/>
                 <labelstyle name="label" font="serif" fontsize="10" fontcolor="0x000000"
                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
                 <labelstyle name="tip" font="serif" fontsize="10" fontcolor="0x000000"
                    linecolor="0x000000" fillcolor="0xFFFFC1" linesize="2"/>
          </valuelinestyle>
          <valueregionstyle name="valueregionstyle">
              <linestyle name="line" color="0x000000" size="1" opacity="1"/>
              <regionstyle name="region" opacity="0"/>
              <labelstyle name="label" font="serif" fontsize="12" fontcolor="0x000000"/>
              <labelstyle name="tip" font="serif" fontsize="12" fontcolor="0x000000"/>
          </valueregionstyle>
          <datastylelist name="datastyles">
               <datastyle>
                    <pointstyle name="point" width="10" height="10" color="0xFF9900" shape="triangle"/>
                    <linestyle name="line" color="0x0000ff" size="3" opacity="1"/>
                    <regionstyle name="region" color="0xff0000" opacity="0.8" gradient="radial"/>
                    <labelstyle name="label" font="serif" fontsize="9" fontcolor="0x0000FF"/>
                    <labelstyle name="tip" font="myverity" fontsize="9" fontcolor="0xFF9900" linecolor="0xFF9900" linesize="2" fillcolor="0xFFFFC1" opacity="0.8" fontangle="45"/>
               </datastyle>
               <datastyle>
                    <pointstyle name="point" width="8" height="8" color="0x66B3FF" shape="square"/>
                    <linestyle name="line" color="0x000000" size="3" opacity="1" />
                    <regionstyle name="region" color="0x00FF00FF" opacity="0.8" />                    
                    <labelstyle name="label" font="myverity" fontsize="9" fontcolor="0x009900" linecolor="0x009900" fillcolor="0xFFFFC1" linesize="2" opacity="0.8" fontangle="20"/>
                    <labelstyle name="tip" font="myverity" fontsize="9" fontcolor="0xFF0000" />
               </datastyle>         
          </datastylelist>
     </chartstyle>
     
<!--- chartstyle02 used for test_line_02.lzx-->

      <chartstyle name="chartstyle02">
          <plotstyle name="plot"/>
