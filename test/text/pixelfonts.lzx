    <!-- Font tester classes -->

    <class name="fonttester" width="400" oninit="doit()" >
        <simplelayout axis="y" spacing="10" />
        <text name="fn" width="100"> </text>
        <text pixellock="true">
            abcdefghijklmnopqrstuvwxyz1234567890!@#$%^*()-=+|"
        </text>
        <text pixellock="true">
            ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890!@#$%^*()-=+|"
        </text>
        <text pixellock="true">
            <b>abcdefghijklmnopqrstuvwxyz1234567890!@#$%^*()-=+|"</b>
        </text>
        <text pixellock="true">
            <b>ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890!@#$%^*()-=+|"</b>
        </text>
        <text pixellock="true" fgcolor="#00FF00"><font color="#00FF00">
            Green
        </font></text>
        <windowtext pixellock="true">Redmond input text</windowtext>
        <inputtext pixellock="true">Base input text</inputtext>
        <method name="doit" > 
            this.fn.setText(this.fontname);
        </method>

    </class>

    <window title="Fonts are fun!" width="400" height="400" resizable="true" >
