import java.util.ArrayList;

//------------------------------------------------------------------------------
// See examples/javarpc/accentedtext.lzx
//------------------------------------------------------------------------------

public class AccentedTest {

    public void getVoid() { }

    public String getJapaneseChars() {
        return "\u3080 \u30c0 \u30d0 \u30e0 \u30f0 \u30a1 \u30b1 \u30c1 \u30d1 \u30e1 \u30f1 ";
    }

    public String getAccentedChars() {
        return "\u00d0\u00e0\u00f0\u00d1\u00e1\u00f1\u00d2\u00e2\u00f2\u00d3\u00e3\u00f3\u00dc";
    }

    public ArrayList getArrayListOfString() {
        ArrayList list = new ArrayList();
        list.add("\u00d0");
        list.add("\u00e0");
        list.add("\u00f0");
        list.add("\u00d1");
        list.add("\u00e1");
        list.add("\u00f1");
        list.add("\u00d2");
        list.add("\u00e2");
        list.add("\u00f2");
        list.add("\u00d3");
        list.add("\u00e3");
        list.add("\u00f3");
        list.add("\u00dc");
        return list;
    }
}
