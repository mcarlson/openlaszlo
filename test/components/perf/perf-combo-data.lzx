            <textlistitem datapath="itemlist8:/list/item"
                              text="$path{ 'text()' }"
                              value="$path{ '@value' }"/>
        </combobox>
        <combobox editable="false" dataoption="lazy" shownitems="4">
            <textlistitem datapath="itemlist9:/list/item"
                              text="$path{ 'text()' }"
                              value="$path{ '@value' }"/>
        </combobox>
    </view>
