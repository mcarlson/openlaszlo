<canvas debug="true">
    <include href="redmond"/>

<window width="200" height="200"
                name="tryme" title="hi there" />

<window width="200" height="200" fgcolor="teal"
                name="closeme"
                resizable="true" closeable="true"
                x="100" y="200"/>

<window width="200" height="200" x="50" y="50"
    resizable="true" closeable="true"
    id="fw" title="Frosty the Snowman">
    <include href="../testmedia/frosty.lzx" />
    <scrollbar id="sb"/>
</window>

<window name="cherry" title="pixellock" x="200" y="200" pixellock="true">
      <view resource="../testmedia/cherry.png"/>
</window>


<button x="400" label="appear" onclick="closeme.setVisible(true)" />
<button x="400" y="80" label="new simple window" onclick="new window(canvas,{title:'new', name:'anon'})" />

</canvas>
