<canvas debug="true">
    <include href="redmond" />
    <button label="test" onclick="tester.printInfo();"/>
    <view y="40" name="tester" >
        <view width="20" height="20" bgcolor="red" />
        <view x="100" y="120"  width="50" height="120" bgcolor="blue" />
        <method name="printInfo" args="props" >
            Debug.write( "dot x: " + this.x + " getProp x: " + 
                            this.getProp( "x" ) );
            Debug.write( "dot y: " + this.y + " getProp y: " + 
                            this.getProp( "y" ) );
            Debug.write( "dot width: " + this.width + " getProp width: " + 
                            this.getProp( "width" ) );
            Debug.write( "dot height: " + this.height + " getProp height: " + 
                            this.getProp( "height" ) );
        </method>
    </view>
</canvas>
