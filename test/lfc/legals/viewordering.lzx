<canvas width="100%" height="80%">
    <view bgcolor="red" width="100" height="100" onmousedown="this.bringToFront();"/>
    <view bgcolor="blue" x="20" y="20" width="100" height="100" onmousedown="this.bringToFront();"/>
    <view bgcolor="green" x="40" y="40" width="100" height="100" onmousedown="this.bringToFront();"/>
    <view bgcolor="yellow" x="60" y="60" width="100" height="100" onmousedown="this.bringToFront();"/>
        
    <view bgcolor="red" x="100" width="100" height="100" onmousedown="this.sendToBack();"/>
    <view bgcolor="blue" x="120" y="20" width="100" height="100" onmousedown="this.sendToBack();"/>
    <view bgcolor="green" x="140" y="40" width="100" height="100" onmousedown="this.sendToBack();"/>
    <view bgcolor="yellow" x="160" y="60" width="100" height="100" onmousedown="this.sendToBack();"/>


</canvas>
