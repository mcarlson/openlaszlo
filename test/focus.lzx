<canvas width="1024" height="1024">
<include href="redmond"/>
<simplelayout/>

<text>Tab should move through A1-3 if the focus is in An, and
similarly for Bn and Cn.</text>
<text>Shift-TAB should move in the opposite order.</text>

<!-- window is a focus trap -->
<window height="200">
  <simplelayout/>
  <windowtext>A1</windowtext>
  <windowtext>A2</windowtext>
  <windowtext>A3</windowtext>
  <!-- A focus trap within a focus trap -->
  <window width="70">
    <simplelayout/>
    <windowtext>B1</windowtext>
    <windowtext>B2</windowtext>
    <windowtext>B3</windowtext>
  </window>
</window>

<!-- Elements directly on the canvas -->
<windowtext>C1</windowtext>
<windowtext>C2</windowtext>
<windowtext>C3</windowtext>

