<library>

  <class name="myclass" bgcolor="#ffccff" 
	 width="${this.textview.width}"
	 height="${this.textview.height}" fontsize="20">
    <attribute name="text" type="text"/>
    <text name="textview"  resize="true" text="${parent.text}"/>
  </class>

</library>
