<canvas debug="true">
<include href="lzunit"/>
    <script>
    Debug.write("This test file exercises compiler support for CSS names with underscores and dashes.");
    </script>
     
    <stylesheet>
        #gMax {
            lzx_bgcolor: #00FF00;
        }
    </stylesheet>
    
    <debug/>
    
    <class name="B" width="50" height="50" bgcolor="$style{'lzx_bgcolor'}"/>
    
<B id="gMax" />
    
<TestSuite>
    <TestCase>
        <method name="testnamesuite">
                assertEquals('0x00FF00', gMax.bgcolor);
        </method>
    </TestCase> 
</TestSuite>

</canvas>
