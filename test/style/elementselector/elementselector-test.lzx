<library>
    <simplelayout />
    <designerview id="gRobby" />
    <designerview id="gPatrick" />    
    <designerview id="gSteve" />    

    <designerview /> <!-- no id, just plain --> 
    <designerview id="gFindMe" /> 
    <designerview bordercolor="0xABBADD" /> <!-- no id -->

    <testsuite name="elementsuite" loglevel="log">
        <handler name="oninit">
            this.addTest( "testRobby");
            this.addTest("testPatrick");
            this.run();
        </handler>

        <method name="testRobby">
            this.assertNotNull( gRobby );
            this.assertEquals( 279, gRobby.getWidth() );
            this.assertEquals( "uc berkeley", gRobby.getAttribute("miscdata"))
            this.finishedTest("testRobby");
        </method>
        <method name="testPatrick">
            this.assertNotNull( gPatrick );
            this.assertEquals( 279, gPatrick.getWidth() );
            this.assertEquals( "uc berkeley", gPatrick.getAttribute("miscdata"))
            this.finishedTest("testPatrick");
        </method>

    </testsuite>
    
    
</library>