<library>
    <script src="ids_css.js" />
    <include href="designerview.lzx" />
    
    <designerview id="gPhilip" />
    
    <designerview id="cabinet"/>

    <designerview /> <!-- no id, just plain --> 
    <designerview id="gFindMe" /> 
    <designerview bordercolor="0xABBADD" /> <!-- no id -->
        
    <testsuite name="idssuite" loglevel="log">
        <handler name="oninit">
            this.addTest( "testPhilip");
            this.addTest( "testTagTypeAndId");            
            this.run();
        </handler>

        <method name="testPhilip">
            this.assertNotNull( gPhilip );
            this.assertEquals( 50, gPhilip.getWidth() );
            this.assertEquals( "san jose state", gPhilip.getAttribute("miscdata"))
            this.finishedTest("testPhilip");
        </method>
        
        <method name="testTagTypeAndId">
            this.assertEquals( "find me please", gFindMe.getAttribute("miscdata"));            
            this.finishedTest("testTagTypeAndId");
        </method>

    </testsuite>

</library>