<canvas debug="true">
    <dataset name="imagedata">
        <image url="img/food_is_large.jpg"/>
    </dataset>

    <wrappinglayout/>
    <image src="img/food_is_large.jpg"/>
    <image datapath="imagedata:/image/@url"/>
    <image prefix="" src="img/food_is_large.jpg"/>
    <image src="img/food_is_large.jpg"/>
    <image>
        <text>Click me</text>
        <handler name="onclick">
            this.setAttribute('src', 'img/food_is_large.jpg');
        </handler>
    </image>
    <image forcereload="true">
        <text>Click me to reload</text>
        <handler name="onclick">
            this.setAttribute('src', 'img/food_is_large.jpg');
        </handler>
    </image>
</canvas>

