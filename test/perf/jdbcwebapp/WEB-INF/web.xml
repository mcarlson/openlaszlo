<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

    <display-name>PKANG</display-name>
    <description/>

    <servlet>
        <servlet-name>JDBCServlet</servlet-name>
        <servlet-class>JDBCServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>JDBCServlet</servlet-name>
        <url-pattern>/getCategories</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>JDBCServlet</servlet-name>
        <url-pattern>/getHierarchy</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>JDBCServlet</servlet-name>
        <url-pattern>/getUserSettings</url-pattern>
    </servlet-mapping>

</web-app>
