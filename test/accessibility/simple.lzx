<canvas width="100%" height="80%"  accessible="true" focustrap="true">

  

  <simplelayout spacing="6"/>
  <button>television</button>

  <view id='v1' width="50" height="50" bgcolor="#cccccc" focusable="true" aadescription="Vanilla"/>
  <view id='v2' width="50" height="50" bgcolor="red"     focusable="true" aadescription="Chocolate"/>
  <view id='v3' width="50" height="50" bgcolor="blue"    focusable="true" aadescription="Strawberry"/>

            <button height="22">Submit</button>
            <button height="22">Cancel</button>
            <button height="22">OK</button>
            <button height="22">Revert</button>

  <button>focus v1
    <handler name="onclick">
      var d = v1.sprite.__LZdiv;
      d.focus();
    </handler>
  </button>

  <button>focus v2
    <handler name="onclick">
      var d = v2.sprite.__LZdiv;
      d.focus();
    </handler>
  </button>

  <button aadescription="button for v3 focus">focus v3
    <handler name="onclick">
      var d = v3.sprite.__LZdiv;
      d.focus();
    </handler>
  </button>




</canvas>
