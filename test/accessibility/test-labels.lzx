<canvas width="100%" height="80%" debug="true" accessible="true">
  <debug  fontsize="12"/>

  <!-- test the addition of <label> elements for aadescription -->

  <simplelayout/>

  <button id="button1" aadescription="The label for button 1" onclick="this.changeText();" focusable="true">
    Button 1
    <method name="changeText">
      this.setAttribute('aadescription', "This is the dynamically changed label for button 1");
    </method>
  </button>

  <button id="button2" aadescription="The label for button 2" onclick="this.changeText();" focusable="true">
    Button 2
    <method name="changeText">
      this.setAttribute('aadescription', "This is the dynamically changed label for button 2");
    </method>
  </button>




</canvas>
