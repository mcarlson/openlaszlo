<canvas width="1000">
    <simplelayout axis="x"/>
    <resource name="tlogo" src="logo.gif"/>
    <view resource="logo.gif">
        <text>1</text>
    </view>
    <view resource="tlogo">
        <text>2</text>
    </view>
    <view resource="http:logo.gif">
        <text>3</text>
    </view>
    <view resource="file:logo.gif">
        <text>4</text>
    </view>
    <view>
        <method event="onconstruct">
            this.setResource( 'http:logo.gif' );
        </method>
        <text>5</text>
    </view>
</canvas>


