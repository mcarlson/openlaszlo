<canvas width="400" height="300" debug="true">
	<debug y="100"/>
	<class name="test">
		<method name="go" args="where">
			if (!where) return
			debug.write("we're going places")
		</method>
	</class>
	
	<view bgcolor="blue" height="50" width="100">
		<test name="start"/>
		<button onclick="parent.start.go(false)">Print nothing</button>
	</view>
