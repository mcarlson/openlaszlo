<canvas width="1000" height="1000" debug="true">
    <text>You should see the text: "This is some text."</text>
    <view y="100">
        <view id="later" initstage="defer">
            <method name="createChildren" args="c">
                super.createChildren( c );
                this.completeInstantiation();
            </method>
            <text>This is some text.</text>
        </view>
    </view>
</canvas>
