<canvas debug="true">

    <security>
        <allow>
            <pattern>^org\.openlaszlo\.test\.xmlrpc\.SystemProp$</pattern>
        </allow>
    </security>

    <javarpc name="systemprop" scope="none"
             classname="org.openlaszlo.test.xmlrpc.SystemProp">
        <method event="onload">
            Debug.write('proxy loaded');
        </method>
        <method event="onerror" args="msg">
            Debug.write('error:', msg);
        </method>
    </javarpc>

