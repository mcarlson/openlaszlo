<canvas debug="true"> 

    <view name="foo"> 
        <method name="bar" args="smelly"> 
            Debug.write( "Smelly BEFORE: " + smelly ); 

            var smelly = "This is what smelly was set to after"; 

            Debug.write( "Smelly AFTER: " + smelly ); 
        </method> 
    </view> 

    <button onclick="foo.bar('poo')">Show me</button> 
</canvas> 
