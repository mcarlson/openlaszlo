<canvas debug="true">
    <dataset name="weather" src="http://www.laszlosystems.com/cgi-pub/weather.cgi"/>
    
    <datapointer xpath="weather:/*" ondata="Debug.write( this.serialize() );"/>
    
    <simplelayout/>
    <button>Works
        <method event="onclick">
            var p = new LzParam();
            p.addValue( "zip", "10022" );
            canvas.datasets.weather.setQueryString( p.toString() );
            canvas.datasets.weather.doRequest();
        </method>
    </button>

    <button>Broken
        <method event="onclick">
            var p = new LzParam();
            p.addValue( "zip", "80211" );
            canvas.datasets.weather.setQueryString( p );
            canvas.datasets.weather.doRequest();
        </method>
    </button>
    <button>Extra test
        <method event="onclick">
            canvas.datasets.weather.setQueryString({zip: "95656" } );
            canvas.datasets.weather.doRequest();
        </method>
    </button>
</canvas>
