<canvas>
    <debug y="40" height="300"/>
    <class name="ca">
        <method name="sa">
            Debug.write( "ca sa" );
        </method>

        <method name="sb">
            Debug.write( "ca sa" );
        </method>
    </class>
    
    <class name="cb" extends="ca">
        <method name="sa">
            Debug.write( "cb sa" );
            super.sb();
        </method>

        <method name="sb">
            Debug.write( "cb sa" );
            super.sa();
        </method>
    </class>
    <class name="cc" extends="cb">
        <method name="sa">
            Debug.write( "cc sa" );
            super.sb();
        </method>

        <method name="sb">
            Debug.write( "cc sa" );
            super.sa();
        </method>
    </class>

    <cc width="20" height="20" bgcolor="blue" onclick="this.sb()"
       oninit="Debug.write( 'When clicked, debugger should print:\ni sb\ncc sa \ncb sb\nca sa\n****')">
        
        <method name="sa">
            Debug.write( "i sa" );
            super.sb();
        </method>

        <method name="sb">
            Debug.write( "i sb" );
            super.sa();
        </method>
    </cc>

</canvas>
