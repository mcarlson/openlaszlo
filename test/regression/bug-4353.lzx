<canvas debug="true" height="450" width="700">

    <debug x="20" width="600" height="400" />

    <dataset name="googleDset" />

    <soap name="google" wsdl="http://api.google.com/GoogleSearch.wsdl">
        <method event="onload">
            Debug.write('google soap service stub loaded');
            Debug.write('running search query...');
            this.search.invoke();
        </method>

        <method event="onerror" args="error">
            debug.write('error:', error);
        </method>

        <remotecall name="search" funcname="doGoogleSearch" 
                    dataobject="googleDset">

            <param value="'2TKUw4ZQFHJ84ByemZK0EXV0Lj+7xGOx'" />
            <param value="'sweet'" />
            <param value="1" />
            <param value="10" />
            <param value="true" />
            <param value="''" />
            <param value="true" />
            <param value="''" />
            <param value="''" />
            <param value="''" />

            <method event="ondata" args="value">
                debug.clear();
                debug.write('return value is:');
                debug.inspect(value);
                debug.write('typeof(directoryCategories):', typeof(value.directoryCategories));
                debug.write('typeof(documentFiltering):', typeof(value.documentFiltering));
                debug.write('typeof(endIndex):', typeof(value.endIndex));
                debug.write('typeof(estimateIsExact):', typeof(value.estimateIsExact));
                debug.write('typeof(estimatedTotalResultsCount):', typeof(value.estimatedTotalResultsCount));
                debug.write('typeof(resultElements):', typeof(value.resultElements));
                debug.write('typeof(searchComments):', typeof(value.searchComments));
                debug.write('typeof(searchQuery):', typeof(value.searchQuery));
                debug.write('typeof(searchTime):', typeof(value.searchTime));
                debug.write('typeof(searchTips):', typeof(value.searchTips));
                debug.write('typeof(startIndex):', typeof(value.startIndex));
            </method>

        </remotecall>
    </soap>

