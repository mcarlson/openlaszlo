<canvas>
<debug/>
<dataset name="fred"/>
  <method event="oninit"> <![CDATA[
    debug.write("adding two params");
    canvas.fred.setQueryParam("thing1","stuff1");
    canvas.fred.setQueryParam("thing2","stuff2");

    var params = canvas.fred.getParams();
    debug.write("params: ",canvas.fred.getParams().d);
    debug.write("calling setQueryParams(null)");
    canvas.fred.setQueryParams(null);

    debug.write("params: ",canvas.fred.getParams().d);
  ]]>
  </method>
</canvas>
