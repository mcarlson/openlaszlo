<canvas bgcolor="silver" height="500">
  <debug x="300" y="300" height="150" width="200"/>
    <include href="utils/layouts/wrappinglayout.lzx"/>

    <view y="15">
        <simplelayout axis="x" spacing="10" />
        <radiogroup initstage="late" id="r1">
            <radiobutton value="1" text="radiogroup late"/>
            <radiobutton value="2" text="no"/>
            <radiobutton value="3" text="maybe"/>
        </radiogroup>
        <radiogroup id="r2">
            <radiobutton initstage="late" value="1" text="radiobutton late"/>
            <radiobutton initstage="late" value="2" text="two"/>
            <radiobutton initstage="late" value="3" text="three"/>
            <radiobutton initstage="late" value="4" text="four"/>
        </radiogroup>
        <radiogroup initstage="late" id="r3">
            <radiobutton initstage="late" value="1" text="all late"/>
            <radiobutton initstage="late" value="2" text="two"/>
            <radiobutton initstage="late" value="3" text="three"/>
            <radiobutton initstage="late" value="4" text="four"/>
        </radiogroup>
        <radiogroup id="r4">
            <radiobutton value="1" text="2 and 4 late"/>
            <radiobutton initstage="late" value="2" text="two"/>
            <radiobutton value="3" text="three"/>
            <radiobutton initstage="late" value="4" text="four"/>
        </radiogroup>
    </view>
</canvas>
