<canvas debug="true" width="900" height="600">

    <debug x="500" y="20" />

    <view x="20" y="20">

        <simplelayout spacing="20" />

        <view>
            <simplelayout axis="x" spacing="5"/>
            <tabslider id="normaltabslider" spacing="5"  clip="false"
                width="150" height="200" bgcolor="yellow">
                <tabelement bgcolor="red" text="normal" selected="true" />
                <tabelement bgcolor="red" text="two" />
                <tabelement bgcolor="red" text="three" />
                <tabelement bgcolor="red" text="four" />
            </tabslider>

            <tabslider id="one" initstage="late" spacing="5" 
                width="150" height="200" bgcolor="yellow">
                <tabelement bgcolor="red" text="tabslider late" selected="true" />
                <tabelement bgcolor="red" text="two" />
                <tabelement bgcolor="red" text="three" />
                <tabelement bgcolor="red" text="four" />
            </tabslider>

            <tabslider id="two" spacing="5" width="150" height="200" bgcolor="yellow">
                <tabelement initstage="late" bgcolor="red" text="tabelements late" selected="true" />
                <tabelement initstage="late" bgcolor="red" text="two" />
                <tabelement initstage="late" bgcolor="red" text="three" />
                <tabelement initstage="late" bgcolor="red" text="four" />
            </tabslider>

            <tabslider id="three" initstage="late" width="150" height="200" spacing="5" 
                bgcolor="yellow">
                <tabelement initstage="late" bgcolor="red" text="all late" selected="true" />
                <tabelement initstage="late" bgcolor="red" text="two" />
                <tabelement initstage="late" bgcolor="red" text="three" />
                <tabelement initstage="late" bgcolor="red" text="four" />
            </tabslider>

            <tabslider id="four" initstage="late" width="150" height="200" spacing="5" 
                bgcolor="yellow">
                <tabelement bgcolor="red" text="2 and 4 late" selected="true" />
                <tabelement initstage="late" bgcolor="red" text="two" />
                <tabelement bgcolor="red" text="three" />
                <tabelement initstage="late" bgcolor="red" text="four" />
            </tabslider>
        </view>

        <view>
            <simplelayout axis="x" spacing="5"/>
            <basetabslider id="normalbasetabslider" spacing="5" 
                width="150" height="200" bgcolor="yellow">
                <basetabelement name="one" clickable="true" bgcolor="green" selected="true">
                    <text>normal</text>
                </basetabelement>
                <basetabelement name="two" clickable="true" bgcolor="green">
                    <text>two</text>
                </basetabelement>
                <basetabelement name="three" clickable="true" bgcolor="green">
                    <text>three</text>
                </basetabelement>
                <basetabelement name="four" clickable="true" bgcolor="green">
                    <text>four</text>
                </basetabelement>
            </basetabslider>

            <basetabslider id="five" initstage="late" spacing="5" 
                width="150" height="200" bgcolor="yellow">
                <basetabelement name="one" clickable="true" bgcolor="green" selected="true">
                    <text>basetabslider late</text>
                </basetabelement>
                <basetabelement name="two" clickable="true" bgcolor="green">
                    <text>two</text>
                </basetabelement>
                <basetabelement name="three" clickable="true" bgcolor="green">
                    <text>three</text>
                </basetabelement>
                <basetabelement name="four" clickable="true" bgcolor="green">
                    <text>four</text>
                </basetabelement>
            </basetabslider>

            <basetabslider id="six" spacing="5" width="150" height="200" bgcolor="yellow">
                <basetabelement name="one" initstage="late" clickable="true" bgcolor="green" selected="true">
                    <text>basetabelements late</text>
                </basetabelement>
                <basetabelement name="two" initstage="late" clickable="true" bgcolor="green">
                    <text>two</text>
                </basetabelement>
                <basetabelement name="three" initstage="late" clickable="true" bgcolor="green">
                    <text>three</text>
                </basetabelement>
                <basetabelement name="four" initstage="late" clickable="true" bgcolor="green">
                    <text>four</text>
                </basetabelement>
            </basetabslider>

            <basetabslider id="seven" initstage="late" spacing="5" width="150" height="200" bgcolor="yellow">
                <basetabelement name="one" initstage="late" clickable="true" bgcolor="green" selected="true">
                    <text>all late</text>
                </basetabelement>
                <basetabelement name="two" initstage="late" clickable="true" bgcolor="green">
                    <text>two</text>
                </basetabelement>
                <basetabelement name="three" initstage="late" clickable="true" bgcolor="green">
                    <text>three</text>
                </basetabelement>
                <basetabelement name="four" initstage="late" clickable="true" bgcolor="green">
                    <text>four</text>
                </basetabelement>
            </basetabslider>

            <basetabslider id="eight" spacing="5" width="150" height="200" bgcolor="yellow">
                <basetabelement name="one" clickable="true" bgcolor="green" selected="true">
                    <text>2 and 4 late</text>
                </basetabelement>
                <basetabelement name="two" initstage="late" clickable="true" bgcolor="green">
                    <text>two</text>
                </basetabelement>
                <basetabelement name="three" clickable="true" bgcolor="green">
                    <text>three</text>
                </basetabelement>
                <basetabelement name="four" initstage="late" clickable="true" bgcolor="green">
                    <text>four</text>
                </basetabelement>
            </basetabslider>

        </view>

    </view>

