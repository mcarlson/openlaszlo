      <view height="1" bgcolor="black"  width="100%"/>
</class>

<tabslider height="200" bgcolor="blue" width="200"
    oninit="this.openTab(this.one, false)"
    mintabheight="20"
    slideduration="300">
    <mytab name="one"/>
    <mytab/>
    <mytab/>

</tabslider>

