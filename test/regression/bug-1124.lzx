<canvas debug="true" >

  <class name="RSSfeedMgr">
    <attribute name="sourceFile" type="string" />
    <attribute name="dsource" type="expression" />
    <attribute name="ds" type="expression" />
    <attribute name="dp" type="expression" />
    <method event="processData">
      <![CDATA[
        debug.write("Entered .processData()");
      ]]>
    </method>
    <method event="showError">
      debug.write("Dataset error processing " + this.sourceFile + ".  Error = " +
                  this.ds.getErrorString());
    </method>
    <method name="build">
      <![CDATA[
        this.dsource = new Datasource(this);
        this.ds = new LzHTTPDataset( this, { name: this.sourceFile, 
                                             type: "http",
                                             autorequest: false } );

        this.ds.initialize(this.dsource);
        var xp = this.sourceFile + ":/"
        this.dp = new LzDatapointer( this, { ondata: this.processData, 
                                           onerror: this.showError,
                                           xpath: xp } );
        ds.setSrc(this.sourceFile);
        debug.write("executing doRequest()");
        ds.doRequest();
      ]]>
    </method>
  </class>  

  <RSSfeedMgr name="feedMgr" sourceFile="../resources/xml/syndi8.xml"></RSSfeedMgr>
  <script>
    feedMgr.build();
  </script>


</canvas>


