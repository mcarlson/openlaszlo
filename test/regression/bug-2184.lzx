<canvas debug="true">
    <debug height="300"/>
    <view>
        <method event="onconstruct">
            <![CDATA[
            Debug.write( 'log1');
            Debug.write( '<log2>');
            ]]>
        </method>
        <method event="oninit">
            <![CDATA[
            Debug.write( 'log3');
            Debug.write( '<log4>');
            ]]>
        </method>
    </view>
</canvas>
