<canvas>
<include href="redmond"/>
   <window title="My Window" name="childWindow" x="30"  y="70" width="265" 
            height="200" resizable="true" closeable="true" fgcolor="#ccc">
        <menubar placement="menubar">
             <menu label="File" hasbutton="true" width="180">
                <menuitem label="New" height="16"  />
                <menuitem label="open..." height="16" />
                <menuitem label="Close" height="16"/>
                <menuseparator/>
                <menuitem label="Save" height="16" />
            </menu>
            <menu label="Edit" hasbutton="true" width="150">
                <menuitem label="Cut"   height="16" />
                <menuitem label="Copy"  height="16" />
                <menuitem label="Paste" height="16" />
            </menu>
        </menubar>
        <view x="500" y="500" />
 
        <scrollbar axis="x"/>
        <scrollbar axis="y"/>

    </window>
</canvas>
