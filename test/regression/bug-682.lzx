
<canvas width="800" height="800" bgcolor="0xDDCCFF"> 
    <class name="parentImage" width="200" height="200" resource="../resources/gif/halle.gif">
        <view name="childImage" x="50" y="50" width="134" height="167" resource="../resources/gif/halle.gif" />
    </class>

    <view name="testView" x="20" y="20" width="600" height="600" bgcolor="#FFFFFF" oninit="this.createImages()">
        <method name="createImages">
                var img = new parentImage( this, {x:20, y:20});
                img.childImage.setSource("../resources/gif/buckwheat.gif");
        </method>
    </view>
