<canvas debug="true">
        <method name="onkeydown" event="onkeydown" reference="LzKeys" >
            keys = LzKeys.downKeysArray;
            for(i = 0; i &lt; keys.length; i++) {
                Debug.write("key " + keys[i] + " down" );
        </method>
