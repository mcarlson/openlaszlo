<canvas>
    <window title="default button test" width="300" height="200">
        <checkbox id="c">switch</checkbox>
        <view layout="axis:x; spacing:20" align="right" valign="bottom">
            <button isdefault="${c.value}">Cancel</button>
            <button isdefault="${!c.value}">OK</button>
        </view>
    </window>
</canvas>
