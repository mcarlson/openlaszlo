<canvas bgcolor="yellow" width="600" height="500">
	<include href="redmond"/>
	<simplelayout/>
	<window fgcolor="#ccc" width="400" height="200" title="Menubar has placement attribute">
		<menubar height="30" placement="menubar">
			<menu label="File" hasbutton="true" width="180">
				<menuitem label="New" height="16"  />
				<menuitem label="Open" height="16" />
				<menuitem label="Save" height="16"/>
				<menuseparator/>
				<menuitem label="Close" height="16" />
			</menu>
		</menubar>
	</window>

	<window fgcolor="#ccc" width="400" height="200" title="Menubar has no placement attribute">
		<menubar height="30">
			<menu label="File" hasbutton="true" width="180">
				<menuitem label="New" height="16"  />
				<menuitem label="Open" height="16" />
				<menuitem label="Save" height="16"/>
				<menuseparator/>
				<menuitem label="Close" height="16" />
			</menu>
		</menubar>
	</window>
