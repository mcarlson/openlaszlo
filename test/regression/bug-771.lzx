    <resource name="starButton" src="../resources/swf/star_button.swf" />

    <view name="button1" x="10" y="10" resource="starButton"
          onmouseover="this.setResourceNumber( 3 )"
          onmouseout="this.setResourceNumber( 1 )"
          onmousedown="this.setResourceNumber( 2 )"
          onmouseup="this.setResourceNumber( 1 )"
          onclick="canvas.button2.setSource( '../resources/swf/star_button.swf' )" />

    <view name="button2" x="50" y="10"/>
