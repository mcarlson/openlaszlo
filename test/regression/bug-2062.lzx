<canvas debug="true">
    <method event="oninit">
        canvas.setWidth ( 100 );
        canvas.setHeight ( 100 );
        canvas.setX ( 100 );
        canvas.setY ( 100 );
        Debug.write( canvas.x );
        Debug.write( canvas.y );
        Debug.write( canvas.width );
        Debug.write( canvas.height );
    </method>
</canvas>
