<canvas debug="true">

    <!-- debugger window should say "commanded" whenever shift and N are
         pressed together -->
    <command onselect="Debug.write( 'commanded' )"
                 key="['N','Shift']"/>

    <method event="onkeydown" reference="LzKeys" args="k">
        Debug.write( 'down: ' + k );
    </method>
    <method event="onkeyup" reference="LzKeys" args="k">
        Debug.write( 'up: ' + k );
    </method>
</canvas>
