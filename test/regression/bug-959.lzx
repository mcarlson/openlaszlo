<canvas>
  <resource name="two" src="../resources/gif/2c.gif"/>

  <class name="card" width="73" height="97" extends="view" 
  	 resource="two" oninit="discard.doStart();">
    <animatorgroup name="discard" process="simultaneous"
		    duration="1000">
          <animator attribute="y" to="200"/>
          <animator attribute="x" to="100"/>
          <animator attribute="rotation" to="180"/>
    </animatorgroup>
</class>

<card name="card1" x="200" y="50"/>
