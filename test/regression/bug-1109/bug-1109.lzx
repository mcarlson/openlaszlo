<canvas debug="true">
    <dataset name="ds1" type="http" src="twoheaders.jsp" />
    <datapointer xpath="ds1:/*">
        <method event="ondata">
            Debug.write( "got headers: " );
            Debug.write( this.getDataset().getAllResponseHeaders().
                                            serialize( ":" , "\n" ));
        </method>
    </datapointer>
    <button onclick="ds1.doRequest()"/>
</canvas>
