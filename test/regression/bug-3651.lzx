<canvas>

    <include href="lz/tree.lzx" />

    <class name="mytree" extends="tree" open="true" />

    <mytree x="20" y="20">top
        <mytree>A
            <mytree closechildren="true">Close immediate children
                <mytree closesiblings="true" open="false">Close sibling
                    <mytree>D
                        <mytree isleaf="true">E</mytree>
                    </mytree>
                </mytree>
                <mytree>C2
                    <mytree>D2
                        <mytree isleaf="true">E2</mytree>
                    </mytree>
                </mytree>
            </mytree>
        </mytree>
        <mytree closechildren="true">Closes all children 
            <mytree closechildren="true">two
                <mytree closechildren="true">three
                    <mytree isleaf="true">four</mytree>    
                </mytree>    
            </mytree>    
        </mytree>
    </mytree>

</canvas>
