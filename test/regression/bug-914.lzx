<canvas debug="true">
    <debug y="100"/>
    <class name="myclass">
        <attribute name="foo" setter="this.foo = foo + bar"/>
        <attribute name="bar" value="2"/>
        <method event="oninit">
            this.setAttribute( 'foo', 10 );
            Debug.write( "foo=" + this.foo );
        </method>
    </class>

    <text> Should print "foo=12" then "foo=20" to the debugger</text>
    <myclass />
    <myclass bar="10"/>

</canvas>
