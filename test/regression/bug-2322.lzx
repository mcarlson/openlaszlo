<canvas debug="true">
	<class name="dubious" defaultplacement="'content'">
		<view name="content">
			<method event="onaddsubview" args="v">
				debug.write("+:"+v);
			</method>
			<method name="addsubview" args="s">
				debug.write("addsubview",s);
				super.addsubview(s);
			</method>
		</view>
	</class>
	
	<dubious id="d">
		<view/>
		<view>
			<view/>
		</view>
	</dubious>
