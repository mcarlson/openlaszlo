<canvas>


    <class name="mybutton" bgcolor="yellow" width="100" height="22">
        <attribute name="text" type="text"/>
        <!-- the x position of the button text. default: centered -->
        <attribute name="text_x"
            value="${this.width/2 - this._title.width/2 -1}" type="number"/>

        <text name="_title"
                x="${parent.text_x}"
                y="4"
                text="${parent.text}"
                resize="true"/>

    </class>


    <mybutton id="b1" height="22" align="right" width="90" text_x="25" text="Overview">

    </mybutton>

    <mybutton id="b2" height="22" align="right" width="90"  y="50">
           <state name="openOverview" apply="true" >
               <attribute name="text" value="Overview"
                          type="string" />
               <attribute name="text_x" value="25"
                          type="number" />

           </state>
    </mybutton>

    <mybutton id="b3" height="22" align="right" width="90"  y="75" text="Overview">
           <state name="openOverview" apply="true" >
               <attribute name="text_x" value="25"/>

           </state>
    </mybutton>

