<canvas width="800" height="600">
  <debug y="200" height="300" width="700" />
  <window title="My Window" name="childWindow" x="30" y="30"
          width="265" height="160" resizable="true" opacity="1"
          closeable="true" bgcolor="#ccc" >

    <menubar width="100%" placement="menubar">
      <menu text="File" width="180">
        <menuitem text="New" />
        <menuitem text="open..."/>
        <menuitem text="Close"/>
        <menuseparator/>
        <menuitem text="Save"/>
      </menu>
      <menu text="Edit" width="150">
        <menuitem text="Cut"/>
        <menuitem text="Copy" />
        <menuitem text="Paste" />
      </menu>
    </menubar>

    <!-- Laszlo Animation swf -->
    <view width="100%" height="100%" >
      <view x="50" y="20" id="v"/>
      <scrollbar/>
    </view>

    <text>
      this is some text
    </text>
    
  </window>
</canvas>
