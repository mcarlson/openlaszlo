<canvas debug="true" width="1024" height="768">
  <include href="lzunit" />

  <script>
    Debug.setAttribute('height', 400);
    Debug.setAttribute('width', 600);
    Debug.setAttribute('x', 0);
    Debug.setAttribute('y', 200);
    Debug.setLine(0);
  </script>

  <class name="TestBreak" extends="TestCase">
    <method name="testBreak">
      var k = 0;
      for (var j in [1, 2, 3, 4, 5])
        for (var i in [1, 2, 3, 4, 5]) {
          if (i == j)
            break;
          k++;
        }
      assertEquals(0+1+2+3+4, k, "expected number of iterations");
    </method>

    <method name="testContinue">
      var k = 0;
      for (var j in [1, 2, 3, 4, 5])
        for (var i in [1, 2, 3, 4, 5]) {
          if (i == j)
            continue;
          k++;
        }
      assertEquals(4*5, k, "expected number of iterations");
    </method>

  </class>

  <TestSuite>
    <TestBreak />
  </TestSuite>

</canvas>
