<canvas width="1400" height="600"  medialoadtimeout="1000" proxied="false" debug="true">
  <debug width="800" height="600" fontsize="12" x="400"/>

  <method event="oninit">
    Debug.write("The icons on the two buttons at the bottom of each column ('Write Message' and 'Check Mail') should fill in sequentially after a delay of five seconds each time.");
  </method>


    <include href="classes/buttons.lzx" />
     
    <class name="testiconbutton" extends="iconbutton">
        <attribute name="fpath" value="classes/resources/icons/"/>
        <attribute name="iconstretches" value="none"/>
    </class>

    <class name="foobar">
      <simplelayout/>
        <text>SIZE 23</text>
        <testiconbutton iconname="search.swfxxx" texty="4"
                    text="Search" width="70" />
        
        <testiconbutton name="reply"
                    iconname="reply.swfxxx"
                    text="Reply" 
                    width="60" />
                    
        <testiconbutton iconname="forward.swfxxx"
                    text="Forward" 
                    width="80" />
                    
        <testiconbutton iconname="spam.swfxxx"
                    text="This Is Spam" 
                    width="95" />
                    
        <testiconbutton iconname="print.swfxxx"
                    text="Print" 
                    width="60" />

        <testiconbutton iconname="delete.swfxxx"
                    text="Delete" 
                    width="65 " />
                    

        <testiconbutton iconname="writemessage.swf"
                    texty="6"
                    text="Write Message" 
                    width="110" />
                    
        <testiconbutton iconname="checkmail.swf"
                    texty="6"
                    text="Check Mail" 
                    width="110" />
    </class>


        <view layout="axis:x">
          <foobar/>
          <foobar/>
          <foobar/>
          <foobar/>
          <foobar/>
        </view>

    

</canvas>
