<canvas debug="true">
    <class name="edittest" extends="edittext"
              focusable="true" >

        <method event="onkeydown" args="k">
            Debug.write( "onkeydown sent: " + k );
        </method>

        <method name="doEnterUp">
            debug.write("doEnterUp");
            //super.doEnterUp();
        </method>

        <method name="doEnterDown">
            debug.write("doEnterDown");
            //super.doEnterUp();
        </method>
    </class>

    <simplelayout spacing="10" inset="10"/>
    <view layout="axis:x;spacing:10">
        <text>doesenter:</text>
        <edittest doesenter="true" x="5" width="100"/>
    </view>
    <view layout="axis:x;spacing:10">
        <text>doesn't enter:</text>
        <edittest x="5" width="100"/>
    </view>
</canvas>
