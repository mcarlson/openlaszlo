<canvas layout="axis:y;spacing:10" width="800">
    <debug y="100"/>
    <resource name="resource5">
        <frame src="icons/mcu_one.swf" />
        <frame src="icons/mcu_two.swf" />
        <frame src="icons/mcu_three.swf" />
     </resource>

    <resource name="resource6" src="icons/mc_123.swf"/>


    <view layout="spacing:4">
        <text>v5: swf - nested movie clips with unique names (instance and clip)</text>
        <view id="v5" resource="resource5"/>
        <view layout="axis:x; spacing:5">
            <button onclick="v5.setAttribute('frame', 1)">1</button>
            <button onclick="v5.setAttribute('frame', 2)">2</button>
            <button onclick="v5.setAttribute('frame', 3)">3</button>
        </view>
    </view>

    <view layout="spacing:4">
        <text>v6: swf - multiframe swf with nested movie clips</text>
        <view id="v6" resource="resource6"/>
        <view layout="axis:x; spacing:5">
            <button onclick="v6.setAttribute('frame', 1)">1</button>
            <button onclick="v6.setAttribute('frame', 2)">2</button>
            <button onclick="v6.setAttribute('frame', 3)">3</button>
        </view>
    </view>
</canvas>