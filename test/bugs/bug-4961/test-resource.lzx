<canvas layout="class:wrappinglayout;axis:x;spacing:10" width="800">
    <debug y="100"/>
    <resource name="resource1">
        <frame src="icons/icon_rurf_read.png" />
        <frame src="icons/icon_rurf_unread.png" />
        <frame src="icons/icon_rurf_reply.png" />
     </resource>

    <resource name="resource2">
        <frame src="icons/opened_email.swf" />
        <frame src="icons/opened.swf" />
        <frame src="icons/arrow_left_replied.swf" />
     </resource>

    <resource name="resource3">
        <frame src="icons/one.swf" />
        <frame src="icons/two.swf" />
        <frame src="icons/three.swf" />
     </resource>

    <resource name="resource4">
        <frame src="icons/mc_one.swf" />
        <frame src="icons/mc_two.swf" />
        <frame src="icons/mc_three.swf" />
     </resource>

    <resource name="resource5">
        <frame src="icons/mcu_one.swf" />
        <frame src="icons/mcu_two.swf" />
        <frame src="icons/mcu_three.swf" />
     </resource>

    <resource name="resource6" src="icons/mc_123.swf"/>

    <view layout="spacing:4">
        <text>v1: png</text>
        <view id="v1" resource="resource1"/>
        <view layout="axis:x; spacing:5">
            <button onclick="v1.setAttribute('frame', 1)">1</button>
            <button onclick="v1.setAttribute('frame', 2)">2</button>
            <button onclick="v1.setAttribute('frame', 3)">3</button>
        </view>
    </view>

    <view layout="spacing:4">
        <text>v2: swf - nested movie clip</text>
        <view id="v2" resource="resource2"/>
        <view layout="axis:x; spacing:5">
            <button onclick="v2.setAttribute('frame', 1)">1</button>
            <button onclick="v2.setAttribute('frame', 2)">2</button>
            <button onclick="v2.setAttribute('frame', 3)">3</button>
        </view>
    </view>

    <view layout="spacing:4">
        <text>v3: swf - simple</text>
        <view id="v3" resource="resource3"/>
        <view layout="axis:x; spacing:5">
            <button onclick="v3.setAttribute('frame', 1)">1</button>
            <button onclick="v3.setAttribute('frame', 2)">2</button>
            <button onclick="v3.setAttribute('frame', 3)">3</button>
        </view>
    </view>

    <view layout="spacing:4">
        <text>v4: swf - nested movie clip</text>
        <view id="v4" resource="resource4"/>
        <view layout="axis:x; spacing:5">
            <button onclick="v4.setAttribute('frame', 1)">1</button>
            <button onclick="v4.setAttribute('frame', 2)">2</button>
            <button onclick="v4.setAttribute('frame', 3)">3</button>
        </view>
    </view>

    <view layout="spacing:4">
        <text>v5: swf - nested movie clips with unique names (instance and clip)</text>
        <view id="v5" resource="resource5"/>
        <view layout="axis:x; spacing:5">
            <button onclick="v5.setAttribute('frame', 1)">1</button>
            <button onclick="v5.setAttribute('frame', 2)">2</button>
            <button onclick="v5.setAttribute('frame', 3)">3</button>
        </view>
    </view>

    <view layout="spacing:4">
        <text>v6: swf - multiframe swf with nested movie clips</text>
        <view id="v6" resource="resource6"/>
        <view layout="axis:x; spacing:5">
            <button onclick="v6.setAttribute('frame', 1)">1</button>
            <button onclick="v6.setAttribute('frame', 2)">2</button>
            <button onclick="v6.setAttribute('frame', 3)">3</button>
        </view>
    </view>
</canvas>