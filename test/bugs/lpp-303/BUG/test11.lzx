<canvas debug="true">
  <debug
      y="10%" height="85%" 
      oninit="LzLoadQueue.__LZmonitorState = true; LzLoader.__LZmonitorState = true;" />

  <dataset name="dsResources" src="resources.xml"/>

  <view 
      datapath="dsResources:/resources/logos/"
      resource="${'http:../resources/'+fn}" >
    <attribute 
        name="fn" 
        value="$path{'employeeLogo0/text()'}" >
    </attribute>
    <method event="ontimeout">
      Debug.write("timeout", this);
    </method>
    <method event="onerror">
      Debug.write("error", this);
    </method>
  </view>
</canvas>
