<canvas debug="true">
    <include href="lzunit"/>
    
    <TestSuite>
        <TestCase>
            <method name="testSplit">
                var str = "Over the Hills and Far Away"
                var tok = str.split("and")
                
                assertEquals(2, tok.length)
                assertEquals(str, tok.join("and"))
                
                assertEquals(2, str.split('H').length)
            </method>
        </TestCase>
    </TestSuite>
