<!--
    Evaluation order test
-->

<canvas width="1024" height="768">
    <include href="lzunit/lzunit.lzx" />

    <script>
        var a = 42;
        function f () {
            a = 3;
            return function (x) { return x };
        }
    </script>

    <class name="EvaluationOrder" extends="TestCase">
        <method name="testFunctionCall">
            assertEquals(3, f()(a))
        </method>
    </class>

    <TestSuite>
        <EvaluationOrder />
    </TestSuite>
</canvas>
