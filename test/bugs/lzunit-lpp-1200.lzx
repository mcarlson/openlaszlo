<canvas debug="true">
  <include href="lzunit"/>

  <class name="ac">
    <attribute name="defaultplacement" value="subc" type="string"/>
    <view name="suba"/>
    <view name="subb"/>
    <view name="subc"/>
    <view name="subd"/>
  </class>

  <TestSuite>
    <TestCase>
      <ac name="ac">
        <datapointer name="dpA">
          <attribute name="placement" value="suba" type="string"/>
        </datapointer>
        <datapointer name="dpB">
          <attribute name="placement" value="subb" type="string"/>
          <attribute name="ignoreplacement" value="false"/>
        </datapointer>
        <datapointer name="dpC">
        </datapointer>
      </ac>
      
      <method name="testPlacement">
        assertEquals(ac.dpA, ac.suba.dpA);
        assertEquals(ac.dpB, ac.subb.dpB);
        assertEquals(ac.dpC, ac.subc.dpC);
      </method>
      
    </TestCase>
  </TestSuite>
</canvas>
