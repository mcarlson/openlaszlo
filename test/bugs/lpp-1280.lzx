<canvas>
  <drawview width="100" height="100">
    <method event="oninit">
        var rpw = 20;
        this.beginPath();
        this.arc( rpw, rpw, rpw, 0, 90, true);
        this.closePath();
        this.strokeStyle = 0x000000;
        this.fillStyle = 0xFF0000;
        this.stroke();
        this.fill(); 
    </method>
  </drawview>
</canvas> 
