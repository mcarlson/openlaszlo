<canvas>
<include href="debugger"/>
<script>
function f() {
  var value = 'value';
  for (var i in [1,2,3,4,5])
    if (i == 3)
      break;
  Debug.write("returning " + value);
  return value;
}
function g() {
  var value = f();
  Debug.write("received " + value);
}
g();
</script>
</canvas>
