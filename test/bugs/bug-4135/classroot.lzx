<!-- This should be smaller than when inline-only="false", and clicking the view should print c1 in the debugger. -->

<canvas debug="true">
  <?lzc class="c1" inline-only="true"?>

  <debug x="100"/>
  
  <class name="c1">
    <method name="f">Debug.write("c1")</method>
    <view width="10" height="10" bgcolor="red" onclick="classroot.f()"/>
  </class>
  
  <c1/>
