<!-- Clicking in the upper right corner should print the name of the view,
     not undefined. -->

<canvas>
  <?lzc classes="c1" inline-only="true"?>
  
  <class name="c1"/>
  
  <c1 id="myid" width="10" height="10"
      onclick="Debug.write('myid =', myid)"/>
</canvas>
