<!-- This should get a compilation error, because the inlined class would call
     the setter before the children are added, unlike the un-inlined class. -->

<canvas>
  <?lzc classes="c1 c2" inline-only="true"?>
  
  <class name="c1">
    <attribute name="a1" value="1"/>
  </class>
  
  <class name="c2" extends="c1">
    <attribute name="a1" setter="Debug.write('v1 =', v1)"/>
    <view name="v1"/>
  </class>
  
  <c2/>
</canvas>
