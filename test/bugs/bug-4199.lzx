    <include href="lzunit"/>
    
    <script>
        var f = false
    </script>

    <method event="onconstruct">
        f = true
    </method>

    <class name="bug_4199" extends="TestCase">
        <method name="testOnconstruct">
            assertTrue(f, "canvas.onconstruct not sent!")
        </method>
    </class>
    
    <TestSuite>
        <bug_4199/>
    </TestSuite>
    
