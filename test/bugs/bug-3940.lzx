<canvas layout="axis:x; spacing:10; inset:20">
    <debug x="150" height="300"/>
    <constantlayout axis="y" value="20"/>
    <combobox shownitems='4'>
        <textlistitem text="one"/>
        <textlistitem text="two"/>
        <textlistitem text="three"/>
        <textlistitem text="four"/>
        <textlistitem text="five"/>
	<method event="onkeyup" args="kc">
            debug.write('onkeyup', kc);
        </method>
        <method event="onkeydown" args="kc">
            debug.write('onkeydown', kc);
        </method>
        <method event="onfocus" args="v">
            debug.write('onfocus', v);
        </method>
        <method event="onblur" args="v">
            debug.write('onblur', v);
        </method>
    </combobox>

    <combobox shownitems='4' editable="false">
        <textlistitem text="one"/>
        <textlistitem text="two"/>
        <textlistitem text="three"/>
        <textlistitem text="four"/>
        <textlistitem text="five"/>
        <method event="onkeyup" args="kc">
            debug.write('onkeyup', kc);
        </method>
        <method event="onkeydown" args="kc">
            debug.write('onkeydown', kc);
        </method>
        <method event="onfocus" args="v">
            debug.write('onfocus', v);
        </method>
        <method event="onblur" args="v">
            debug.write('onblur', v);
        </method> 
    </combobox>

    <list id="me" shownitems='4'>
        <textlistitem text="one"/>
        <textlistitem text="two"/>
        <textlistitem text="three"/>
        <textlistitem text="four"/>
        <textlistitem text="five"/>
        <method event="onkeyup" args="kc">
            debug.write('onkeyup', kc);
        </method>
        <method event="onkeydown" args="kc">
            debug.write('onkeydown', kc);
        </method>
        <method event="onfocus" args="v">
            debug.write('onfocus', v);
        </method>
        <method event="onblur" args="v">
            debug.write('onblur', v);
        </method> 
    </list>

</canvas>
