<canvas>
<text id="txt" initstage="early" bgcolor="yellow" width="200" height="200">
   <method name="write" args="t">
       this.setText(this.text + "\r" + t);
    </method>
</text>

<view id="v1" oninit="txt.write('v1 oninit')"/>
<view id="v2" oninit="txt.write('v2 (early) oninit')" initstage="early"/>
<dataset name="ds1" type="http" oninit="txt.write('ds1 oninit')" />
<dataset name="ds2" type="http" oninit="txt.write('ds2 (early) oninit')" initstage="early"/>
<view id="v3" oninit="txt.write('v3 oninit')"/>
<view name="n1">
  <dataset name="ds3" initstage="early" oninit="txt.write('ds3 (early) oninit')" />
  <dataset name="ds4" />
  <dataset oninit="txt.write('ds5 oninit')" />
</view>

<datasource>
  <dataset name="ds6" initstage="early" oninit="txt.write('ds6 (early) oninit')" />
</datasource>

<connectiondatasource>
  <dataset name="ds7" initstage="early" oninit="txt.write('ds6 (early) oninit')" />
</connectiondatasource>

</canvas>

