<canvas debug="true">
  <include href="lzunit"/>
  <class name="constraintTest" extends="TestCase">
    <attribute name="ff" type="number" value="0"/>
    <text name="yy" text="bar"/>
    <attribute name="hexff" type="string" value="${'hex'+ff.tostring(16)}"/>
    <attribute name="hexbar" type="string" value="${'hex'+yy.getText()}"/>
    <attribute name="hexfoo" type="string"/>
   <method event="onff">
      hexfoo='hex'+ff.toString(16);
    </method>
    <method name="testCalls">
      setAttribute('ff',255);
      yy.setText('255');
      assertEquals('ff',ff.tostring(16));
      assertEquals('hexff',hexff);
      assertEquals('hexff',hexfoo);
      assertEquals('hex255',hexbar);
      debug.write(hexff);
    </method>
  </class>

  <TestSuite>
    <constraintTest/>
  </TestSuite>

</canvas>
