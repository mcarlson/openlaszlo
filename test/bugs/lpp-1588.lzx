<canvas>
  <drawview width="100" height="100">
    <method event="oninit">
      beginPath();
      rect(0, 0, 100, 100);
      arc(50, 50, 50, 0, 2*Math.PI, true);
      stroke();
    </method>
  </drawview>
</canvas> 
