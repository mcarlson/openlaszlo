<canvas debug="true">

    <resource name="mail_folders_rsc" src="folders.swf"/>

    <simplelayout spacing="20" inset="20" />
    <constantlayout axis="x" value="20" />

    <view layout="spacing: 10">

        <text>
        Clicking on button should replace resource with runtime resource.
        </text>

        <view id="icn1" resource="mail_folders_rsc" />

        <button text="set source on icn1">
            <method event="onclick">
                icn1.setSource('inbox.swf');
            </method>
        </button>

    </view>

</canvas>
