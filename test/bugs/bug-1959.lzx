<canvas debug="true">
    <inputtext name="ttext"/>
    <button oninit="Debug.write( 'Button should print -- to the debugger')">
        <method event="onclick">
            ttext.setText( undefined );
            Debug.write( "-"+ttext.getText( null )+"-" );
        </method>
    </button>
</canvas>
