<canvas>
    <class name="roundrect" extends="drawview">
        <method event="oninit">
        this.drawStructure();
        </method>
        <method name="drawStructure">
                var w = this.width;
                var h = this.height;
                var yc = this.height/2;
                var rpw = 10;

                // draw a rectangle around the whole area
                this.beginPath();
                this.rect(0, 0, w, h, rpw);
                this.strokeStyle = 0x000000;
                this.lineWidth = 1;
                this.stroke();
        </method>
    </class>
    <roundrect x="10" y="10" height="100" width="100"/>
    <roundrect x="120.5" y="10.5" height="100" width="100"/>
</canvas>
