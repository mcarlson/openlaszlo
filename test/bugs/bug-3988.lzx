<canvas width="800" height="800" debug="true">

    <debug x="300" y="20" width="500" height="400" />

    <include href="rpc/sessionrpc.lzx" />

    <dataset name="idDset" />

    <javarpc name="counter" scope="session"
        classname="org.openlaszlo.test.xmlrpc.PeerCounter">

        <method event="oncreate">
            Debug.write('counter proxy created');
        </method>        

        <method event="onremove">
            Debug.write('removed counter proxy:', this.proxy);
        </method>

        <method event="onerror" args="error">
            Debug.write("onerror:", error);
        </method>

    </javarpc>


    <button text="remove proxy" onclick="counter.remove()" />

</canvas>
