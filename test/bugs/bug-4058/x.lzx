<canvas debug="true">
  <dataset name="data" src="x.xml" />
  <simplelayout axis="y" />
  <view datapath="data:/data/string">
  <simplelayout axis="x" />
  <text datapath="@id" />
  <text datapath="text()">
    <method name="applydata" args="d">
       setText(d.length)
      </method>
  </text>
  </view>
</canvas>
