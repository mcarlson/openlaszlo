            Debug.write('error:', error);
        </method>
    </soap>

    <soap name="anotherlps" wsdl="file:multiservice.wsdl" 
          service="AnotherTestService" port="LPSTest">
        <method event="onload">
            Debug.write('reverse (another/lps):', this.proxy.reverse);
        </method>
        <method event="onerror" args="error">
            Debug.write('error:', error);
        </method>
    </soap>

</canvas>
