<canvas width="1400" height="600" >
  <debug width="400" height="300" fontsize="12" x="400"/>

  <class name="myclass">
    <attribute name="formdata"/>
    <method event="onconstruct">
      Debug.write('myclass onconstruct ', this);
      this.formdata = {};
    </method>
    <method event="oninit">
      Debug.write('myclass oninit ', this);
    </method>
  </class>


  <import name="snippet" href="snippet.lzx" stage="late" />  

  <method event="onload" reference="snippet">
    new c4 ( canvas, { id: "gDetails", x: "20", y: "20", 
                       bgcolor: 0xffcccc, visible: true, width: "100", height: "100" } );

    var v = new LzText( canvas, 
    { x: 0, y: 500, 
      width: 500, height: 100, multiline: true, 
      bgcolor: "0x00FF00", fgcolor: "0xFF0000", 
      text: "parent = " + gDetails.contents + "\nformdata = " + gDetails.contents.formdata } );
  </method>

  <button/>


  
</canvas>

