<canvas width="1400" height="600" debug="true">
  <debug width="400" height="300" fontsize="12" x="400"/>

  <include href="lz/tree.lzx"/>
  <dataset name='sumo' type='http' request='false'
           src='SUMO.xml'/>

  <view layout="axis:y; spacing:10">
    <button text='get data' 
            onclick='canvas.datasets.sumo.doRequest()'/>
    <view x='20' y='20' height='200' clip='true'>
      <tree datapath="sumo:/rdf_RDF/rdfs_Class/@rdf_ID"/>
      <vscrollbar/>
    </view>
  </view>

</canvas>
