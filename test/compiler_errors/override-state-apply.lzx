<canvas>
  <class name="mystate" extends="state">
    <method  name="apply">
      Debug.write('mystate apply method');
    </method>
    <attribute  name="y" value="200"/>
  </class>
  
  <mystate/>
<!-- should NOT generate compiler warning that 'apply' method is overwriting
     the 'apply' attribute on the parent -->
</canvas>
