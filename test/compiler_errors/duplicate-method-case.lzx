<!-- A methd is defined with the same name as an attribute. Compiler warning should be issued -->

<canvas width="300" height="200" bgcolor="red"> 
  <view>
    <method name="foo">
       Debug.write("this is the first foo method");
    </method>

    <method name="Foo" args="n">
       Debug.write("this is the second foo method, with arg "+n);
    </method>
  </view>
</canvas>
