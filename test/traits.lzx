<canvas>

    <include href="../lps/components/lzunit"/>

    <class name="testClass1" traits="'ta'">
        <attribute name="foo" value="1"/>
    </class>

    <trait bar="7">
        <attribute name="name" value="9"/>
        <attribute name="foo" value="2"/>

        <method name="grag">
            this.plop = 99;
        </method>
    </trait>

    <TestSuite>
        <TestCase>
            <method name="test1">
                var itC1 = new testClass1();
                assertEquals( itC1.foo , 2 );
            </method>
        </TestCase>
    </TestSuite>
